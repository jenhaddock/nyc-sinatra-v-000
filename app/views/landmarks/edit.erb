<h1>Update <%=@landmark.name%></h1>

<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch">

  Name: <input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>">
  Year Completed: <input type="text" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed%>">

  <br></br>

  <%Title.all.each do |t|%>
    <input type="checkbox" name="landmark[title_ids][]" id="<%=t.name%>" value="<%=t.id%>"><%=t.name%></input>
  <%end%>
  <br></br>

  <input type="submit" value="Edit Landmark">
</form>
